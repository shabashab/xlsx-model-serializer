{
  "$id": "/TableMarkup",
  "type": "object",
  "properties": {
    "rows": {
      "type": "object",
      "patternProperties": {
        "\\d+": {
          "$ref": "/RowMarkup"
        }
      }
    },
    "columns": {
      "type": "object",
      "patternProperties": {
        "\\d+": {
          "$ref": "/ColumnMarkup"
        }
      }
    }
  },
  "required": ["rows", "columns"],
  "additionalProperties": false
}
